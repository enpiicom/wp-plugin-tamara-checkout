{
  "name": "tamara-checkout",
  "version": "2.0.0",
  "description": "Tamara Checkout Package",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --watch --mode development",
    "build-blocks": "webpack --config webpack-blocks.config.js --mode production",
    "dev-blocks": "webpack --config webpack-blocks.config.js --watch --mode development",
    "wp-start": "wp-scripts start",
		"wp-build": "wp-scripts build && npm run i18n:build",
		"i18n": "npm run i18n:build",
		"i18n:build": "npm run i18n:pot && ./bin/build_i18n.sh",
		"i18n:pot": "php -d xdebug.max_nesting_level=512 $(which wp) i18n make-pot --exclude=\"node_modules/,languages/,assets/\" --headers='{\"Report-Msgid-Bugs-To\":\"https://woocommerce.com/my-account/create-a-ticket/\", \"language-team\":\"LANGUAGE <EMAIL@ADDRESS>\"}' . languages/woocommerce-gateway-dummy.pot",
		"i18n:json": "$(which wp) i18n make-json languages --no-purge",
		"packages-update": "wp-scripts packages-update",
		"check-engines": "wp-scripts check-engines"
  },
  "author": "dev@tamara.co",
  "license": "ISC",
  "keywords": [
    "Tamara Checkout"
  ],
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "grunt": "^1.1.0",
    "grunt-contrib-cssmin": "^3.0.0",
    "grunt-uncss": "^0.9.1",
    "jquery": "^3.5.1",
    "purgecss": "^2.1.0",
    "purgecss-webpack-plugin": "^2.1.0",
    "purify-css": "^1.2.5",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.54.7",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack-build-notifier": "^2.0.0",
    "@woocommerce/dependency-extraction-webpack-plugin": "^2.2.0",
		"@wordpress/scripts": "^26.10.0",
		"cross-env": "7.0.3",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "file-loader": "^6.2.0",
    "i": "^0.3.6",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  }
}
